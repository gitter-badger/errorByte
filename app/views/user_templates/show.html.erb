<div class="container">
	ID: {{current_user.id}}

	<div class="row text-center">
		<h1>{{users.user.username}}</h1>
		{{users.complete}}

	</div> <!-- Row Close -->

<!-- 	Show All the Beginner Code Titles, but only limit to 10, since 1-10 is beginner code -->
	<div class="row text-center">
		<div class="col-md-6">
			<h3>Beginner</h3>
			<div ng-repeat="beginnerCode in users.code | limitTo: 10">
				{{beginnerCode.title}}
			</div>
		<input type="text" ng-model="users.one">

		</div>

<!-- 	Show All the Beginner Code Titles, but only 11 - 20 for Intermediate code -->
		<div class="col-md-6">
			<h3>Intermediate</h3>
			<div ng-repeat="intermediateCode in users.code" ng-if="$index > 9 && $index <20">
				{{intermediateCode.title}}
			</div>
		</div>		
	</div> <!-- Row Close -->
	
</div> <!-- Container Close -->